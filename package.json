{
  "name": "AwesomeTSProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format:check": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "setup": "rm -rf node_modules && npm install && cd ios && rm -rf Pods  && pod install",
    "run-ios-dev": "ENVFILE=.env.development npx react-native run-ios --scheme \"Development\"  --configuration Debug --simulator=\"iPhone 11\"",
    "run-ios-staging": "ENVFILE=.env.staging npx react-native run-ios --scheme \"Staging\"  --configuration Debug --simulator=\"iPhone 11\"",
    "run-ios-prod": "ENVFILE=.env.production npx react-native run-ios --scheme \"Production\"  --configuration Debug --simulator=\"iPhone 11\"",
    "build-ios-dev": "cd ios && xcodebuild archive -workspace AwesomeTSProject.xcworkspace -scheme Development -archivePath ~/desktop/Development.xcarchive",
    "build-ios-prod": "cd ios && xcodebuild archive -workspace AwesomeTSProject.xcworkspace -scheme Production -archivePath ~/desktop/Production.xcarchive",
    "build-ios-staging": "cd ios && xcodebuild archive -workspace AwesomeTSProject.xcworkspace -scheme Staging -archivePath ~/desktop/Staging.xcarchive",
    "run-android-dev": "ENVFILE=.env.development npx react-native run-android --variant=developmentDebug",
    "run-android-staging": "ENVFILE=.env.staging npx react-native run-android --variant=stagingDebug",
    "run-android-prod": "ENVFILE=.env.production npx react-native run-android --variant=productionDebug",
    "build-android-dev": "cd android && ENVFILE=.env.development ./gradlew app:assembledevelopmentRelease",
    "build-android-staging": "cd android && ENVFILE=.env.staging ./gradlew app:assemblestagingRelease",
    "build-android-prod": "cd android && ENVFILE=.env.production ./gradlew app:assembleproductionRelease",
    "clear-build-android": "cd android ./gradlew clean && ./gradlew cleanBuildCache"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.3.2",
    "@react-navigation/drawer": "^6.4.3",
    "@react-navigation/native": "^6.0.11",
    "@react-navigation/native-stack": "^6.7.0",
    "react": "18.0.0",
    "react-native": "0.69.1",
    "react-native-config": "^1.4.6",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.69.1",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
